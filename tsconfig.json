{
  "compilerOptions": {
    "outDir": "./dist/",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react"
  },
  "exclude": [
    ".vscode",
    "dist",
    "doc",
    "node_modules"
  ]
}
